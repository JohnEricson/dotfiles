# Alias definitions.
# Loaded by .bashrc.

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

alias cm='chezmoi'
alias vi='nvim.appimage'

# Enable to tail rotating log files. Makes tail follow file name instead of file descriptor.
alias tailf='tail --follow=name --retry'

alias h5="head -n 5"

